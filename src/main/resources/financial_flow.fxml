<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.FinancialFlowController">
   <children>
      <BarChart fx:id="barChart" layoutX="14.0" layoutY="13.0" prefHeight="427.0" prefWidth="675.0">
        <xAxis>
          <CategoryAxis fx:id="categoryAxis" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="numberAxis" side="LEFT" />
        </yAxis>
      </BarChart>
      <Button layoutX="55.0" layoutY="440.0" mnemonicParsing="false" onAction="#debitClick" text="Debit" />
      <Button layoutX="106.0" layoutY="440.0" mnemonicParsing="false" onAction="#creditClick" text="Credit" />
      <Button layoutX="645.0" layoutY="440.0" mnemonicParsing="false" onAction="#backClick" text="Back" />
   </children>
</AnchorPane>
